@model StoreBinding

<div class="row">
	<h2 class="jumbotron col-md-offset-3 col-md-6 SysAdminDeletePageTitle text-danger">Confirm Store Binding Delete</h2>
</div>
<div class="row">
	<h3 class="col-md-offset-4 col-md-8 text-danger title-align">Are you sure you want to delete <br />Store Binding Id [@(Model.StoreBindingId)]?</h3>
