@model GStore.Models.ViewModels.RegisterViewModel
@using GStore.AppHtmlHelpers
@using GStore.Data
@using GStore.Models

@{
	StoreFront storeFront = Html.CurrentStoreFront(true);
	if ((storeFront.RegisterWebForm != null) && storeFront.RegisterWebForm.IsActiveBubble())
	{
		List<WebFormField> fields = storeFront.RegisterWebForm.WebFormFields.AsQueryable().WhereIsActive().ToList();
		foreach (WebFormField item in fields)
		{
			@Html.DisplayFor(model => item);
		}
	}
}

