@model GStore.Exceptions.GStoreErrorInfo
@{
	string title = "Error " + Model.ErrorCode + " - Page Not Found";
	ViewBag.Title = title;
}
<h1 class="text-danger">@title</h1>
<h2 class="text-danger">
	Sorry, we could not find the page you are looking for.<br />
	<a href="~/">Click here for the home page</a>
</h2>
<p>@title</p>
@if (Request.IsLocal || User.IsInRole("SystemAdmin"))
{
	@Html.Partial("_ErrorDetailsPartial", Model)
}
