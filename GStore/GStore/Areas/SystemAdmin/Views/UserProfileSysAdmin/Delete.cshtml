@model GStore.Models.UserProfile
@using GStore.AppHtmlHelpers
@using GStore.Data
<div class="row">
	<h2 class="col-md-offset-3 col-md-6 jumbotron SysAdminDeletePageTitle text-danger">Confirm User Profile Delete</h2>
</div>
<div class="row">
	<h3 class="col-md-offset-4 col-md-8 text-danger text-left">
		Are you sure you want to delete User Profile  <br />'@(Model.FullName)' &lt;@(Model.Email)&gt; [@(Model.UserProfileId)]
</h3>
</div>
<div class="row">
	<div class="form-actions col-md-offset-4 col-md-8 form-actions no-color text-left">
		@using (Html.BeginForm())
		{
			@Html.AntiForgeryToken()
			<input type="submit" value="Delete" class="btn btn-danger" />
			@:&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;
			@Html.ActionLink("Cancel", "Index", null, new { @class = "btn btn-default" })
			@:&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;
			@Html.ActionLink("Edit", "Edit", new { id = Model.UserProfileId }, new { @class = "btn btn-warning" })
			@:&nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;
			@Html.ActionLink("Details", "Details", new { id = Model.UserProfileId}, new { @class = "btn btn-success" });
		}
	</div>
</div>
