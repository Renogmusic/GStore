@model GStore.Models.Client
@using GStore.Data
@if (Model == null)
{
	@:(no client)
}
else
{
	bool isActive = Model.IsActiveDirect();
	string className = isActive ? string.Empty : "text-warning";
	<span class="@className">
		'@(Model.Name)' [@(Model.ClientId)] @(isActive ? string.Empty : " [INACTIVE]")
	</span>
}

