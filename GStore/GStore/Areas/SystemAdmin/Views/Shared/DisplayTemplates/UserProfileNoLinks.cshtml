@model GStore.Models.UserProfile
@if (Model == null)
{
	@:(no user profile)
}
else
{
	@:@(Model.UserName) '@(Model.FullName)'
	if (Model.StoreFrontId.HasValue)
	{
		@: @Html.DisplayFor(up => up.StoreFront, "StoreFrontNoLinks")
	}
	else
	{
		@: @Html.DisplayFor(up => up.Client, "ClientNoLinks")
	}
}
