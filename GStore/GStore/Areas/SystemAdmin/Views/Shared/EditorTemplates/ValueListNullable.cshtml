@model ValueList
<div class="form-group">
	@Html.LabelForModel(new { @class = "col-md-2 title-align-right" })
	<div class="col-md-10 title-align">
		@Html.DropDownListForModel(Html.ValueListSelect(), "[No Value List]", true)
		@Html.HelpLabelPopoverForModel()
		<br />
		@Html.ValidationMessageForModel()
	</div>
</div>